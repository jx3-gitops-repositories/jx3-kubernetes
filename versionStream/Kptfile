metadata:
  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
    commit: a0f7050a84f09ccef343a46efe857523f833c85e
