  name: versionStream
upstream:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
  updateStrategy: resource-merge
upstreamLock:
  type: git
  git:
    repo: https://github.com/jenkins-x/jx3-versions
    directory: /
    ref: master
    commit: d7334653df04f701f6e96c7d689634874c887903
